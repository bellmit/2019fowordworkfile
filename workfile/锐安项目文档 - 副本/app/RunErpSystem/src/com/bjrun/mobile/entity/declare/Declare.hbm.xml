<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="true" default-lazy="true"
	package="com.bjrun.mobile.entity.declare">
	<class name="Declare" table="app_wx_declare">
		<id name="id" column="id">
			<generator class="native"></generator>
		</id>
		<property name="userName" column="user_name"></property>
		<property name="userId" column="user_id"></property>
		<property name="wxUserId" column="wx_user_id"></property>
		<property name="isAgree" column="is_agree"></property>
		<property name="createTime" column="create_time"></property>
	</class>
</hibernate-mapping>
