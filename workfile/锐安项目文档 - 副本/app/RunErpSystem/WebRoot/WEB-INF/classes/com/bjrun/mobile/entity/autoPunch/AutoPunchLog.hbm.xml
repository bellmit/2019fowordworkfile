<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.bjrun.mobile.entity.autoPunch">
	<class name="AutoPunchLog" table="app_wx_auto_punch_log">
		<id name="id" column="id">
			<generator class="native"></generator>
		</id>
		<property name="run_id" column="run_id"></property>
		<property name="user_name" column="user_name"></property>
		<property name="descript" column="descript"></property>
		<property name="update_time" column="update_time"></property>
	</class>
</hibernate-mapping>
