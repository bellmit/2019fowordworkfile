<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.bjrun.mobile.entity.user">
	<class name="User" table="sys_user">
		<id name="id" column="id">
			<generator class="uuid"></generator>
		</id>

		<property name="runId" column="run_id"></property>
		<property name="userName" column="username"></property>
		<property name="passWord" column="password"></property>
		<property name="muserId" column="mUserId"></property>
		<property name="wxUserId" column="wxUserId"></property>
		<property name="auto_punch" column="auto_punch"></property>

	</class>
</hibernate-mapping>

