<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[SERVER]]></DatabaseName>
</Connection>
<Query>
<![CDATA[
select  a.cinvsn sn号码,a.cCusCode 客户编码,a.cCusName 客户名称,a.dDate 订单时间,a.cInvCode 存货料号,a.cInvName 存货名称,a.cSOCode 订单号,a.first_bind_timestamp 激活时间,a.prodsp 产品标准价格,a.portsp 端口标准价格,a.pre 折扣,a.iTaxUnitPrice/0.7 价格, a.amount 云服务分摊费用,(a.iTaxUnitPrice/0.7)*0.3-a.amount 软件价格 from  hs_load_order_data_hardwareservice a 
WHERE
	cast( date_format( dDate, '%Y%m' ) AS signed ) between ${startperiod} and ${endperiod} 
	-- 取开始日期小于等于最大条件日期，且结束日期大于等于最小条件日期的数据
	order by dDate]]></Query>
<PageQuery>
<![CDATA[]]></PageQuery>
</TableData>
