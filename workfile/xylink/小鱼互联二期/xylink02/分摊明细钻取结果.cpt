<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[SERVER]]></DatabaseName>
</Connection>
<Query>
<![CDATA[select cinvsn, date_format( use_time, '%Y%m' ) period ,cCusCode,cCusName,cInvCode,cInvName,`port` typename, amount ftaxsum,ftaxnowsum,ftaxperiod,amount-ftaxnowsum ftaxleft,use_time,first_bind_timestamp,cSOCode from hs_report  
WHERE
    1=1
	-- 取开始日期小于等于最大条件日期，且结束日期大于等于最小条件日期的数据
	${if(len(period)=0,"","and date_format( use_time, '%Y%m' )='"+period+"'" )}
	 ${if(len(typename)=0,"","and port= '"+typename+"'" )}
	 ${if(len(custname)=0,"","and cCusName= '"+custname+"'" )}
	 ${if(len(productname)=0,"","and cInvName= '"+productname+"'" )}
	order by first_bind_timestamp]]></Query>
