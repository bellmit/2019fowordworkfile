    <sql>
SELECT
-- replace( replace( a.cexternalItemId, b.ccode, '' ), 'CRM', '' ), -- 从cexternalItemId字段分解出订单行ID，以便关联订单行的金额
-- a.cexternalItemId,
-- a.iproduct,现有数据充值卡产品和订单行上产品不能对应，原因有两种：1、加盟包，2、错误数据
-- e.iproduct,
d.cname custname,
b.ccode ordercode,
c.cname productname,
-- a.fquantity,					-- 商品的数量
-- a.fcycle,						-- 商品的服务期限
-- a.ccycleunit,				-- 商品的单位
a.ccardNum,
e.ftaxsum ,-- 该充值码对应的订单行的“折后金额”
'CRM'  source 
FROM
	crm_sailian.dbo.sc_rechargecard a
	LEFT JOIN crm_sailian.dbo.sc_order b ON b.iid= a.iorder
	LEFT JOIN crm_sailian.dbo.cs_customer d ON d.iid= a.icustomer
	LEFT JOIN crm_sailian.dbo.SC_orders e ON e.iid= replace( replace( a.cexternalItemId, b.ccode, '' ), 'CRM', '' ) -- 关联订单行，以便select时取订单行的折后金额
	LEFT JOIN crm_sailian.dbo.SC_product c ON c.iid= e.iproduct 
WHERE
	a.ifuncregedit != 512 
	AND e.iproduct!= 174 -- 排除样机加盟包
	AND e.iproduct != 114  -- 排除样机加盟包
UNION ALL
SELECT
	-- a.cexternalItemId,
	-- a.iproduct,
	-- e.iproduct,
	d.cname custname,
	b.ccode ordercode,
	c.cname productname,
	a.ccardNum ,
	0,
	'CRM-无订单'  source 
FROM
	crm_sailian.dbo.sc_rechargecard a
	LEFT JOIN crm_sailian.dbo.sc_tryproduct b ON b.iid= a.iorder	
	LEFT JOIN crm_sailian.dbo.cs_customer d ON d.iid= a.icustomer 
	LEFT JOIN crm_sailian.dbo.sc_tryproducts e on e.iid= replace( replace( a.cexternalItemId, b.ccode, '' ), 'CRM', '' ) -- 关联订单行，以便select时取订单行的折后金额
	LEFT JOIN crm_sailian.dbo.SC_product c ON c.iid= e.iproduct
WHERE
	a.ifuncregedit = 512 -- 非512的为订单充值卡

	
	UNION all
	
	SELECT
	-- a.iproduct,
	-- e.iproduct,
	d.cname custname,
	b.ccode ordercode,
	c.cname productname,
	a.ccardNum,
	e.ftaxsum, -- 该充值码对应的订单行的“折后金额”
	'DMS'  source
	
FROM
	dms_sailian.dbo.sc_rechargecard a
	LEFT JOIN dms_sailian.dbo.sc_order b ON b.iid= a.iorder	
	LEFT JOIN dms_sailian.dbo.as_corporation d ON d.iid= b.icustomer
	LEFT JOIN dms_sailian.dbo.SC_orders e ON e.iid= replace( replace( a.cexternalItemId, b.ccode, '' ), 'DMS', '' ) -- 关联订单行，以便select时取订单行的折后金额
	LEFT JOIN dms_sailian.dbo.SC_product c ON c.iid= e.iproduct
WHERE
	a.ifuncregedit = 392 -- 392的为订单充值卡
	


UNION all
SELECT
	-- a.iproduct,
	-- e.iproduct,
	d.cname custname,
	b.ccode ordercode,
	c.cname productname,
	a.ccardNum,
	0,
	'DMS-无订单'  source

	
FROM
	dms_sailian.dbo.sc_rechargecard a
	LEFT JOIN dms_sailian.dbo.sc_secondment b ON b.iid= a.iorder	
	LEFT JOIN dms_sailian.dbo.as_corporation d ON d.iid= b.icustomer
	LEFT JOIN dms_sailian.dbo.sc_secondments e ON e.iid= replace( replace( a.cexternalItemId, b.ccode, '' ), 'DMS', '' ) -- 关联订单行，以便select时取订单行的折后金额
	LEFT JOIN dms_sailian.dbo.SC_product c ON c.iid= e.iproduct

WHERE
	a.ifuncregedit = 1019 -- 1019的为试用订单，目前只有392和1019两种产生充值卡
	and e.iproduct is not null --目前有错误数据iproduct为空，排除掉

</sql>
