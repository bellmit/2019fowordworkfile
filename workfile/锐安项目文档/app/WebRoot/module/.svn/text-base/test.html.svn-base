<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="initial-scale=1.0, user-scalable=no, width=device-width">
    <title>逆地理编码</title>
    <link rel="stylesheet" href="http://cache.amap.com/lbs/static/main1119.css"/>
    <script type="text/javascript" src="http://webapi.amap.com/maps?v=1.3&key=1bd747249cad2f541cb7c6cae5224f41&plugin=AMap.Geocoder"></script>
    <script type="text/javascript" src="http://cache.amap.com/lbs/static/addToolbar.js"></script>
    
    <script type="text/javascript" src="js/jquery.js"></script>
    
</head>
<body onload="regeocoder()">
<div id="container"></div>
<div id="tip">
    <b>经纬度 116.396574, 39.992706 的地理编码结果:</b>
    <span id="result"></span>
</div>
<script type="text/javascript">
     //Ajax调用处理
     function regeocoder(){
     	$.ajax({     
            //要用post方式      
            type: "post",     
            //方法所在页面和方法名      
            url: "http://restapi.amap.com/v3/geocode/regeo?output=json&location=116.310003,39.991957&key=1bd747249cad2f541cb7c6cae5224f41&radius=1000&extensions=all",     
            contentType: "charset=utf-8",     
            dataType: "json",  
            success: function(data) {
                //返回的数据用data.d获取内容      
                alert(data.status);
                alert(data.info);
                if(data.status == 1 && data.info == 'OK'){
                	alert(JSON.stringify(data));
                }
            },     
            error: function(err) {     
                alert(err);     
            }     
        }); 
     }
</script>
</body>
</html>