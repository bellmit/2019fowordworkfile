<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="cn.com.img.entity">

	<class name="File" table="fileinfo">
		<id name="fileId" column="fileid">
			<generator class="uuid"></generator>
		</id>
		<property name="fileName"></property>
		<many-to-one name="folder" class="Folder" column="folderId"></many-to-one>
		<property name="lastModified"></property>
		
	</class>
</hibernate-mapping>

