<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.bjrun.mobile.entity.sign">
	<class name="AppClientOpLog" table="app_client_op_log">
		<id name="id" column="id">
			<generator class="uuid"></generator>
		</id>		
		<property name="userId" column="user_id"></property>
		<property name="userName" column="user_name"></property>
		<property name="opCode" column="operation_code"></property>		
		<property name="opTime" column="operation_time"></property>
		<property name="signChannel" column="sign_channel"></property>
		<property name="longitude" column="longitude"></property>
		<property name="latitude" column="latitude"></property>
		<property name="address" column="address"></property>
		<property name="osType" column="os_type"></property>
		<property name="createDate" column="create_date"></property>	
	</class>
</hibernate-mapping>

