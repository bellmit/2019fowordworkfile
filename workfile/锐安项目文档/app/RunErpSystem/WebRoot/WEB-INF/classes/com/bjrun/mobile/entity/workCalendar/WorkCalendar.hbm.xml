<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.bjrun.mobile.entity.workCalendar">
	<class name="WorkCalendar" table="t_work_calendar_rules">
		<id name="id" column="id">
			<generator class="native"></generator>
		</id>
		<property name="work_hour" column="work_hour"></property>
		<property name="period" column="period"></property>
		<property name="multiple" column="multiple"></property>
		<property name="work_type" column="work_type"></property>
	</class>
</hibernate-mapping>

