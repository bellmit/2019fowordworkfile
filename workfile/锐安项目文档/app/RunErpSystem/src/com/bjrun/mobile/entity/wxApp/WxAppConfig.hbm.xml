<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="true" default-lazy="true"
	package="com.bjrun.mobile.entity.wxApp">
	<class name="WxAppConfig" table="app_wx_config">
		<id name="id" column="id">
			<generator class="native"></generator>
		</id>
		<property name="wx_app_key" column="wx_app_key"></property>
		<property name="wx_app_value" column="wx_app_value"></property>

	</class>
</hibernate-mapping>
