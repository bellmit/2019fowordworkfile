    <connection>NEWSYS-YJT_HPM</connection>
    <sql>select 
 a.pro_id as id                                       -- 项目id
,a.pro_approval_time  as lx_time                      -- 立项时间
,year(a.pro_approval_time) as z_year                  -- 年度【例：2019】
,quarter(a.pro_approval_time) as z_quarter            -- 季度【例1】
,DATE_FORMAT(a.pro_approval_time,'%Y-%m') as z_month  -- 月份【2019-12】
,a.pro_type                                           -- 项目类型（1商机拓展;2销售项目;5市场活动项目;6客户关系维护项目）
,a.pro_status                                         -- 项目状态(1草稿，2立项，3结项)
,a.pro_code                                           -- 项目编码P2018001
,a.pro_name                                           -- 项目名称
,b.total_contract_budget as yjhte                     -- 预计合同额
,b.if_signed                                          -- 是否签单
,sysdate() as etl_time 
from hpm_project_info a
left join hpm_project_sale b
on a.pro_id=b.pro_id
where a.pro_code is not null
  and year(a.pro_approval_time)=YEAR(SYSDATE())</sql>
