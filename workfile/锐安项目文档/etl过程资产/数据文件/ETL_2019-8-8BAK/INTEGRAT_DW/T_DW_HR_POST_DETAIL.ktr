    <connection>NEWSYS-YJT_TMS</connection>
    <sql>SELECT 
		 G.POST_ID					-- 岗位ID
		,G.POST_CODE				-- 岗位CODE
		,G.POST_NAME                -- 岗位名称
		,G.POST_LEVEL              	-- 岗位称重级别
		,G.POST_DEPT_ID				-- 所属部门ID
		,G.POST_LOCATION        	-- 常驻地
		,G.POST_DUTY_ID        		-- 岗位所属职位ID
		,G.POST_LEADER_ID      		-- 岗位所属上级岗位ID
		,G.POST_STATUS            	-- 岗位状态【1 正常、0 空岗】
		,G.DUTY_CODE                -- 岗位ID
		,G.DUTY_NAME                -- 岗位名称
		,G.DUTY_CATEGORY_ID  		-- 岗位所属职种ID
		,H.CATEGORY_ID			    -- 职种ID
		,H.CATEGORY_CODE      		-- 职种CODE
		,H.CATEGORY_NAME      		-- 职种名称
		,H.category_type_id 		-- 职类ID
		,H.CATEGORY_TYPE_NAME 		-- 职类名称
        ,sysdate() as etl_time      -- etl时间
FROM(

	SELECT 	
			 M.id AS POST_ID					-- 岗位ID
			,M.code	AS POST_CODE				-- 岗位CODE
			,M.name AS POST_NAME                -- 岗位名称
			,M.level AS POST_LEVEL              -- 岗位称重级别
			,M.dept_id AS POST_DEPT_ID			-- 所属部门ID
			,M.location AS POST_LOCATION        -- 常驻地
			,M.duty_id  AS POST_DUTY_ID         -- 岗位所属职位ID
			,M.leader_id AS POST_LEADER_ID      -- 岗位所属上级岗位ID
			,M.status AS POST_STATUS            -- 岗位状态【1 正常、0 空岗】
			,N.code AS DUTY_CODE                -- 岗位ID
			,N.name AS DUTY_NAME                -- 岗位名称
			,N.category_id AS DUTY_CATEGORY_ID  -- 岗位所属职种ID
	FROM YJT_PMS.p_post M , YJT_PMS.p_duty N
	WHERE 1 = 1 
	AND M.duty_id = N.id

)G,(
	SELECT 
			 X.id AS CATEGORY_ID		  -- 职种ID
			,X.code	AS CATEGORY_CODE      -- 职种CODE
			,X.name AS CATEGORY_NAME      -- 职种名称
			,X.category_type_id 		  -- 职类ID
			,Y.name AS CATEGORY_TYPE_NAME -- 职类名称
