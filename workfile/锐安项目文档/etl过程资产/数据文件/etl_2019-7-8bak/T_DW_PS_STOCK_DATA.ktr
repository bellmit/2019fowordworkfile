    <connection>OLDSYS</connection>
    <sql>SELECT 
       a.z_year
      ,a.z_quarter
      ,SUM(a.total_amount) AS total_amount
      ,SUM(a.exceed_amount) AS exceed_amount
      ,SYSDATE() AS etl_time
  FROM (SELECT 
               YEAR(a.pay_date1) AS z_year
              ,QUARTER(a.pay_date1) AS z_quarter
              ,SUM(a.qmoney) AS total_amount
              ,0 AS exceed_amount
          FROM sd_yszk a
         GROUP BY YEAR(a.pay_date1), QUARTER(a.pay_date1)
        UNION ALL
        SELECT 
               YEAR(a.pay_date1) AS z_year
              ,QUARTER(a.pay_date1) AS z_quarter
              ,0 AS total_amount
              ,SUM(a.qmoney) AS exceed_amount
          FROM sd_yszk a
         WHERE a.pstatus IN ('已立项', '未立项', '待立项')
           AND a.pay_date1 IS NOT NULL
           AND a.pay_date1 &lt;= SYSDATE()
         GROUP BY YEAR(a.pay_date1), QUARTER(a.pay_date1)) a
 WHERE a.z_year = YEAR(SYSDATE())
   AND a.z_quarter = QUARTER(SYSDATE())
 GROUP BY a.z_year, a.z_quarter</sql>
