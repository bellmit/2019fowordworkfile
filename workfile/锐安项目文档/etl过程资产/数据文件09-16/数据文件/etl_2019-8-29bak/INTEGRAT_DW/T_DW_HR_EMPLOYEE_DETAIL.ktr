    <sql>SELECT a.id                                               -- 员工编号
      ,a.username AS staff_name                           -- 员工名称
      ,c.sex                                              -- 员工性别
      ,c.birthday                                         -- 出生日期
      ,CASE                                               
         WHEN YEAR(now()) - YEAR(c.birthday) &lt; 25 THEN     
          '小于25岁'                                       
         WHEN YEAR(now()) - YEAR(c.birthday) >= 25 AND     
              YEAR(now()) - YEAR(c.birthday) &lt; 30 THEN     
          '25(含)-30岁'                                    
         WHEN YEAR(now()) - YEAR(c.birthday) >= 30 AND     
              YEAR(now()) - YEAR(c.birthday) &lt; 35 THEN     
          '30(含)-35岁'                                    
         WHEN YEAR(now()) - YEAR(c.birthday) >= 35 AND     
              YEAR(now()) - YEAR(c.birthday) &lt; 40 THEN     
          '35(含)-40岁'                                    
         WHEN YEAR(now()) - YEAR(c.birthday) >= 40 AND     
              YEAR(now()) - YEAR(c.birthday) &lt; 45 THEN     
          '40(含)-45岁'                                    
         ELSE                                              
          '大于45岁(含)'                                 
       END AS age_group                                   -- 年龄段
      ,YEAR(now()) - YEAR(c.birthday) AS age              -- 年龄
      ,c.education                                        -- 学历
      ,c.native_place                                     -- 出生地
      ,c.entry_date                                       -- 入职日期
      ,CASE                                                
         WHEN YEAR(now()) - YEAR(c.entry_date) &lt; 1 THEN    
          '小于1年'                                        
         WHEN YEAR(now()) - YEAR(c.entry_date) >= 1 AND    
              YEAR(now()) - YEAR(c.entry_date) &lt; 2 THEN    
          '1(含)-2年'                                      
         WHEN YEAR(now()) - YEAR(c.entry_date) >= 2 AND    
              YEAR(now()) - YEAR(c.entry_date) &lt; 3 THEN    
          '2(含)-3年'                                      
         WHEN YEAR(now()) - YEAR(c.entry_date) >= 3 AND    
              YEAR(now()) - YEAR(c.entry_date) &lt; 5 THEN    
          '3(含)-5年'                                      
         WHEN YEAR(now()) - YEAR(c.entry_date) >= 5 AND    
              YEAR(now()) - YEAR(c.entry_date) &lt; 10 THEN   
          '5(含)-10年'                                     
         ELSE                                              
          '10年以上'                                       
       END AS age_job_group                               -- 司龄段 
      ,YEAR(now()) - YEAR(c.entry_date) AS age_job        -- 司龄
      ,a.group_id                                         -- 事业群ID
      ,a.group_name                                       -- 事业群名称
      ,a.first_dept_id                                    -- 一级部门ID
      ,a.first_dept                                       -- 一级部门名称
      ,a.second_dept_id                                   -- 二级部门ID
      ,a.second_dept                                      -- 二级部门名称
      ,d. code AS post_id                                 -- 岗位ID
      ,d. NAME AS post_name                               -- 岗位名称
      ,c.post_level                                       -- 岗位级别
      ,d.post_status AS post_type                         -- 岗位类型
      ,e. code AS duty_code                               -- 职位编码
      ,e. NAME AS duty_name                               -- 职位名称
      ,c.position_level AS duty_level                     -- 职位级别
      ,f. code AS category_code                           -- 职种编码
      ,f. NAME AS category_name                           -- 职种名称
      ,g. code AS category_type_code                      -- 职类编码
      ,g. NAME AS category_type_name                      -- 职类名称
      ,SYSDATE() AS etl_time                              -- etl时间
	    ,a.status                                           -- 在职标识 （1 在职 2 离职）
	    ,t1.leave_status                                    -- 离职标识 （1 主动 2 被动）
		  ,t2.attend_status                                   -- 昨日出勤状况（1 正常）
	    ,t3.zy_status                                       -- 资源池标识（1 是 ）
  FROM (SELECT a.id
              ,a.username
              ,a.post_id
              ,a. status
              ,a.dept_id AS group_id
              ,b.dept_name AS group_name
              ,'' first_dept_id
              ,'' first_dept
              ,'' second_dept_id
              ,'' second_dept
              ,b.parent_id
              ,b.dept_type
          FROM p_user a
          JOIN p_dept b ON a.dept_id = b.id
         WHERE b.dept_type = 0
        UNION ALL
        SELECT a.id
              ,a.username
              ,a.post_id
              ,a. status
              ,c.id AS group_id
              ,c.dept_name AS group_name
              ,a.dept_id AS first_dept_id
              ,b.dept_name AS first_dept
              ,'' second_dept_id
              ,'' second_dept
              ,b.parent_id
              ,b.dept_type
          FROM p_user a
          JOIN p_dept b ON a.dept_id = b.id
          JOIN p_dept c ON b.parent_id = c.id
         WHERE b.dept_type = 1
        UNION ALL
        SELECT a.id
              ,a.username
              ,a.post_id
              ,a. status
              ,d.id AS group_id
              ,d.dept_name AS group_name
              ,c.id AS first_dept_id
              ,c.dept_name AS first_dept
              ,a.dept_id AS second_dept_id
              ,b.dept_name AS second_dept
              ,b.parent_id
              ,b.dept_type
          FROM p_user a
          JOIN p_dept b ON a.dept_id = b.id
          JOIN p_dept c ON b.parent_id = c.id
          JOIN p_dept d ON c.parent_id = d.id
         WHERE b.dept_type = 2) a
  JOIN (SELECT * FROM h_staff_entry_info c) c ON a.id = c.id
  LEFT JOIN p_post d ON a.post_id = d.id
  LEFT JOIN p_duty e ON d.duty_id = e.id
  LEFT JOIN p_duty_category f ON e.category_id = f.id
  LEFT JOIN p_duty_category_type g ON f.category_type_id = g.id
  LEFT JOIN -- 当年离职
  (SELECT 
       DISTINCT staff_name    -- 员工名称
	            ,'1' leave_status           -- 标识
    FROM h_staff_leave_active b          
 WHERE YEAR(apply_date) = YEAR(SYSDATE())
UNION ALL
SELECT 
       DISTINCT staff_name    -- 员工名称
                ,'2' leave_status          -- 标识
  FROM h_staff_leave_unactive b
 WHERE YEAR(b.apply_date) = YEAR(SYSDATE())) t1
 ON a.username=t1.staff_name
 LEFT JOIN -- 昨日出勤
 (SELECT 
      DISTINCT user_id -- 员工ID
	          ,'1' attend_status     -- 标识
  FROM t_append_info a
 WHERE 1 = 1
   AND YEAR(TIME) = YEAR(SYSDATE())
   AND MONTH(TIME) = MONTH(SYSDATE())
   AND DAY(TIME) = DAY(SYSDATE()) - 1
   AND (a.describe1 = '正常' OR a.describe2 = '正常' OR
       a.describe3 = '正常' OR a.describe4 = '正常'))t2
on a.id=t2.user_id
 LEFT JOIN 
  (SELECT 
      distinct b.id      -- 员工编号
	          ,'1' zy_status        -- 标识
  FROM p_post a, p_user b, p_dept c
 WHERE 1 = 1
   AND a.id = b.post_id
   AND a.dept_id = c.id
   AND c.dept_duty LIKE '%资源池%')t3
   ON a.id=t3.id
 ORDER BY a.id</sql>
